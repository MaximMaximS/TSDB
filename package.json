{
  "name": "tsdb",
  "description": "Simple app to keep track of your watched episodes of The Simpsons.",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "prod": "dotenv -e .env.development.local -- npm start",
    "bundle": "ts-node ./utils/bundle.ts",
    "docker:build": "docker build -t maximmaxims/tsdb .",
    "lint": "next lint",
    "format": "prettier --write . --ignore-unknown",
    "prepare": "husky install",
    "generate": "prisma generate",
    "studio": "dotenv -e .env.development.local -- prisma studio",
    "push": "dotenv -e .env.development.local -- prisma db push",
    "reset": "dotenv -e .env.development.local -- prisma migrate reset",
    "migrate": "dotenv -e .env.development.local -- prisma migrate dev",
    "deploy": "dotenv -e .env.production.local -- prisma migrate deploy",
    "episodes": "dotenv -e .env.development.local -- ts-node ./utils/generate-episodes.ts",
    "deploy:episodes": "dotenv -e .env.production.local -- ts-node ./utils/generate-episodes.ts --write",
    "seed": "dotenv -e .env.development.local -- npm run reset && npm run episodes --write && ts-node ./utils/seed.ts",
    "docs": "prisma-docs-generator serve",
    "register": "dotenv -e .env.production.local -- ts-node ./utils/register.ts",
    "deps": "pnpm install --frozen-lockfile && pnpm run generate",
    "builder": "pnpm run build && pnpm run bundle"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@prisma/client": "4.15.0",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-scroll-area": "1.0.4",
    "@radix-ui/react-toast": "1.1.4",
    "@tanstack/react-table": "8.9.2",
    "argon2": "^0.30.3",
    "class-variance-authority": "0.6.0",
    "clsx": "1.2.1",
    "lucide-react": "0.241.0",
    "nanoid": "^4.0.2",
    "next": "13.4.5-canary.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "sharp": "0.32.1",
    "statuses": "^2.0.1",
    "tailwind-merge": "1.13.1",
    "tailwindcss-animate": "1.0.6",
    "use-immer": "0.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.1",
    "@types/fs-extra": "11.0.1",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "@types/statuses": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.1.4",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-tailwindcss": "3.12.1",
    "eslint-plugin-unicorn": "47.0.0",
    "fs-extra": "^11.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prisma": "4.15.0",
    "prisma-docs-generator": "^0.8.0",
    "socks-proxy-agent": "^8.0.1",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  }
}
