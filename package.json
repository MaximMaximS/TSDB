{
  "name": "tsdb",
  "description": "Simple app to keep track of your watched episodes of The Simpsons.",
  "version": "0.4.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "cd dist && dotenv -e ../.env.development.local -- node server.js",
    "prod": "cd dist && dotenv -e ../.env.production.local -- node server.js",
    "bundle": "node ./utils/bundle.mjs",
    "docker:build": "docker build -t maximmaxims/tsdb .",
    "lint": "next lint",
    "format": "prettier --write . --ignore-unknown",
    "prepare": "husky install",
    "generate": "prisma generate",
    "studio": "dotenv -e .env.development.local -- prisma studio",
    "push": "dotenv -e .env.development.local -- prisma db push",
    "reset": "dotenv -e .env.development.local -- prisma migrate reset",
    "migrate": "dotenv -e .env.development.local -- prisma migrate dev",
    "deploy": "dotenv -e .env.production.local -- prisma migrate deploy",
    "episodes": "dotenv -e .env.development.local -- node ./utils/generate-episodes.mjs",
    "deploy:episodes": "dotenv -e .env.production.local -- node ./utils/generate-episodes.mjs --write",
    "seed": "dotenv -e .env.development.local -- npm run reset && npm run push && npm run episodes -- --write && node ./utils/seed.mjs",
    "docs": "prisma-docs-generator serve",
    "register": "dotenv -e .env.production.local -- node ./utils/register.mjs",
    "deps": "pnpm install --frozen-lockfile && pnpm run generate",
    "builder": "pnpm run build && pnpm run bundle"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.1",
    "@prisma/client": "5.0.0",
    "@radix-ui/react-alert-dialog": "1.0.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-dialog": "1.0.4",
    "@radix-ui/react-dropdown-menu": "2.0.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-scroll-area": "1.0.4",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-toast": "1.1.4",
    "argon2": "0.30.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "lucide-react": "0.263.1",
    "nanoid": "4.0.2",
    "next": "13.4.12",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.2",
    "server-only": "0.0.1",
    "sharp": "0.32.4",
    "statuses": "2.0.1",
    "tailwind-merge": "1.14.0",
    "tailwindcss-animate": "1.0.6",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@tsconfig/strictest": "2.0.1",
    "@types/fs-extra": "11.0.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.4.5",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/statuses": "2.0.1",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "autoprefixer": "10.4.14",
    "axios": "1.4.0",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "16.3.1",
    "dotenv-cli": "7.2.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unicorn": "48.0.1",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "prisma": "5.0.0",
    "prisma-docs-generator": "0.8.0",
    "socks-proxy-agent": "8.0.1",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
